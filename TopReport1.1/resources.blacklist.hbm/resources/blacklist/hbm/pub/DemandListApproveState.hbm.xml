<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="resource.bean.blacklist">
    <class name="NsDemandListApproveState" table="cop_demand_approve">
        <meta attribute="sync-DAO">false</meta>
        <cache usage="read-write"></cache>

        <id name="id" type="string" column="id" length="15"/>
        <property name="demand_list_no" type="string" column="demand_list_no" length="30"   />
        <property name="brcode" type="string" column="brcode" length="15"   />
        <property name="approve_state" type="string" column="approve_state" length="1" />
        <property name="approve_passday" type="java.util.Date" column="approve_passday" />
        <property name="approve_user_id" type="string" column="approve_user_id" length="15" />
    </class>
</hibernate-mapping>